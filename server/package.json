{
  "name": "server",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx server.js",
    "build": "tsc",
    "pretest": "echo 'Running tests...' && node -e \"console.log('Node version:', process.version)\"",
    "test": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:unit": "jest --testPathIgnorePatterns=integration --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:verbose": "jest --verbose --detectOpenHandles",
    "test:ci": "jest --coverage --ci --maxWorkers=2",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@google/genai": "^0.13.0",
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.49.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "mammoth": "^1.9.0",
    "mongoose": "^8.14.2",
    "multer": "^1.4.5-lts.2",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.1"
  }
}